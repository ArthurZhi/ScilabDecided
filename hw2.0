//
function y=y(x)     // объявляем функцию y(x)
    y=(2+(sin(x))^2)/(1+x^2)
endfunction

function g=g(x)    // объявляем функцию g(x)
    if x<=0 then 
        g=(3.*x^2)/(1+x^2)
    else 
        g=(1+((2.*x)/(1+x^2)))^(1/2) 
    end
endfunction
x=-2:0.05:2       // область значений аргумента функций
subplot(121)      // создаем матрицу графиков 
plot(x, y)        // строим график y(x)
subplot(122)
plot(x, g)        // строим график g(x)
            
